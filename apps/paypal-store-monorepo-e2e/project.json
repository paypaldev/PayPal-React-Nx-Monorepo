{
  "name": "paypal-store-monorepo-e2e",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "apps/paypal-store-monorepo-e2e/src",
  "targets": {
    "e2e": {
      "executor": "@nx/playwright:playwright",
      "outputs": [
        "{workspaceRoot}/dist/.playwright/apps/paypal-store-monorepo-e2e"
      ],
      "options": {
        "config": "apps/paypal-store-monorepo-e2e/playwright.config.ts"
      }
    },
    "lint": {
      "executor": "@nx/linter:eslint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["apps/paypal-store-monorepo-e2e/**/*.{js,ts}"]
      }
    }
  },
  "implicitDependencies": ["paypal-store-monorepo"]
}
